{
  "name": "factoryfactory",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run inngest:dev\"",
    "build": "next build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "next build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "start": "next start",
    "start:backend": "node dist/backend/index.js",
    "start:frontend": "next start",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "backend:dev": "tsx watch src/backend/index.ts",
    "inngest:dev": "inngest-cli dev",
    "health": "curl -s http://localhost:3001/health | jq",
    "health:all": "curl -s http://localhost:3001/health/all | jq"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "inngest": "^3.24.4",
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "superjson": "^2.2.6",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "inngest-cli": "^0.7.1",
    "postcss": "^8.4.49",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
