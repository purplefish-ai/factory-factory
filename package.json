{
  "name": "factoryfactory",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run inngest:dev\"",
    "build": "next build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "next build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "start": "next start",
    "start:backend": "node dist/backend/index.js",
    "start:frontend": "next start",
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "backend:dev": "tsx watch src/backend/index.ts",
    "inngest:dev": "inngest-cli dev",
    "health": "curl -s http://localhost:3001/health | jq",
    "health:all": "curl -s http://localhost:3001/health/all | jq"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "inngest": "^3.49.3",
    "next": "^16.1.4",
    "node-pty": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "superjson": "^2.2.6",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "inngest-cli": "^1.16.1",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
