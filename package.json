    "open": "^11.0.0",
    "p-limit": "^6.2.0",
    "pidusage": "^4.0.1",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.5.3",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "3.7.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css}": [
      "biome check --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "better-sqlite3",
      "esbuild",
      "node-pty",
      "prisma",
      "protobufjs",
      "sharp"
