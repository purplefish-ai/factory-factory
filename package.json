{
  "name": "factoryfactory",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently -n inngest,backend,next -c magenta,green,blue \"pnpm inngest:dev\" \"wait-on tcp:8288 && pnpm backend:dev\" \"pnpm dev\"",
    "build": "next build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "next build",
    "build:all": "pnpm build:backend && pnpm build:frontend",
    "start": "next start",
    "start:backend": "node dist/src/backend/index.js",
    "start:frontend": "next start",
    "start:all": "concurrently \"pnpm start:frontend\" \"pnpm start:backend\"",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "backend:dev": "tsx watch src/backend/index.ts",
    "inngest:dev": "inngest-cli dev --no-discovery -u http://localhost:3001/api/inngest",
    "health": "curl -s http://localhost:3001/health | jq",
    "health:all": "curl -s http://localhost:3001/health/all | jq",
    "postinstall": "chmod +x node_modules/node-pty/prebuilds/*/spawn-helper 2>/dev/null || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.2.1",
    "inngest": "^3.49.3",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "next": "^16.1.4",
    "next-themes": "^0.4.6",
    "node-pty": "^1.1.0",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^4.5.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "superjson": "^2.2.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "biome check --write"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "esbuild",
      "inngest-cli",
      "node-pty",
      "prisma",
      "protobufjs",
      "sharp"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "inngest-cli": "^1.16.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "wait-on": "^9.0.3"
  }
}
