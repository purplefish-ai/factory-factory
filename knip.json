{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [
        "src/client/main.tsx",
        "src/backend/server.ts",
        "src/backend/migrate.ts",
        "electron/main/index.ts",
        "electron/preload/index.ts"
      ],
      "project": ["src/**/*.{ts,tsx}", "electron/**/*.ts"]
    }
  },
  "paths": {
    "@/*": ["src/*"],
    "@prisma-gen/*": ["prisma/generated/*"]
  },
  "ignore": [
    "src/components/ui/**",
    "src/backend/services/index.ts",
    "src/backend/services/*.service.ts",
    "src/backend/resource_accessors/index.ts",
    "src/backend/clients/index.ts",
    "src/backend/trpc/index.ts",
    "src/backend/trpc/procedures/index.ts",
    "src/backend/domains/*/index.ts",
    "src/backend/domains/**/*/index.ts",
    "src/backend/domains/**/*.service.ts",
    "src/backend/orchestration/*.ts"
  ],
  "ignoreDependencies": [
    "autoprefixer",
    "tw-animate-css",
    "@tailwindcss/typography",
    "@prisma/client",
    "@radix-ui/react-accordion",
    "@radix-ui/react-aspect-ratio",
    "@radix-ui/react-avatar",
    "@radix-ui/react-hover-card",
    "@radix-ui/react-menubar",
    "@radix-ui/react-navigation-menu",
    "@radix-ui/react-slider",
    "embla-carousel-react",
    "input-otp",
    "react-hook-form",
    "recharts",
    "tailwindcss-animate",
    "vaul",
    "@hookform/resolvers",
    "geist",
    "@zed-industries/claude-code-acp",
    "@linear/sdk"
  ],
  "vitest": {
    "entry": ["src/**/*.test.{ts,tsx}", "vitest.config.ts"]
  },
  "storybook": {
    "entry": ["src/**/*.stories.tsx", ".storybook/**/*.{ts,tsx}"]
  }
}
